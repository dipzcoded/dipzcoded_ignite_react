s>\r\n              {newGames.length > 0 ? newGamesGrid : <h1>fetching data....</h1>}\r\n            </Games>\r\n          </Fragment>\r\n        )}\r\n      </AnimateSharedLayout>\r\n    </GameList>\r\n  );\r\n};\r\n\r\nconst GameList = styled(motion.div)`\r\n  display: flex;\r\n  flex-direction: column;\r\n  padding: 0rem 5rem;\r\n  h2 {\r\n    padding: 5rem 0rem;\r\n  }\r\n`;\r\nconst Games = styled(motion.div)`\r\n  min-height: 80vh;\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(500px, 1fr));\r\n  grid-column-gap: 3rem;\r\n  grid-row-gap: 5rem;\r\n`;\r\n\r\nexport default Home;\r\n","C:\\Users\\hp\\Documents\\learning\\programming\\front-end dev\\projects\\ignite-react\\src\\component\\Game.js",[],"C:\\Users\\hp\\Documents\\learning\\programming\\front-end dev\\projects\\ignite-react\\src\\component\\GlobalStyles.js",[],"C:\\Users\\hp\\Documents\\learning\\programming\\front-end dev\\projects\\ignite-react\\src\\component\\GameDetails.js",[],"C:\\Users\\hp\\Documents\\learning\\programming\\front-end dev\\projects\\ignite-react\\src\\utils.js",[],["72","73"],"C:\\Users\\hp\\Documents\\learning\\programming\\front-end dev\\projects\\ignite-react\\src\\component\\Navbar.js",[],"C:\\Users\\hp\\Documents\\learning\\programming\\front-end dev\\projects\\ignite-react\\src\\animation.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":9,"column":18,"nodeType":"80","messageId":"81","endLine":9,"endColumn":23},{"ruleId":"74","replacedBy":"82"},{"ruleId":"76","replacedBy":"83"},"no-native-reassign",["84"],"no-negated-in-lhs",["85"],"no-unused-vars","'popup' is defined but never used.","Identifier","unusedVar",["84"],["85"],"no-global-assign","no-unsafe-negation"]